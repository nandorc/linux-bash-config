#!/bin/bash

# Load dependencies
source ~/.bash_utilities/lib/ext/dsoft/messages.sh

# Manage utility parameters
if [ "$1" = "--help" ]; then
    ~/.bash_utilities/src/mdv/cmd/display.sh ~/.bash_utilities/src/mdv/README.md
else
    printErrorMessage "No valid option provided for mdv." before
    printWarningMessage "Try 'mdv --help' to look for available options." after
fi
